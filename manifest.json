{
  "manifest_version": 2,
  "name": "Relearun",
  "description": "plugging chrome client server",
  "version": "1.0",
  "permissions": [ 
    "http://*/*",
    "https://*/*",
    "webNavigation"
   ],

  "browser_action": {
    "default_icon" : "./img/icon.png",
    "default_popup" : "/popup.html"
  },

  "icons": { 
    "128" : "img/icon_128.png" 
   },


  "background" : {
    "scripts" : ["background.js"],
    "persistent" : true
  }


}